main
{
int num=16;
        double sqrt;
        System.out.println(squareRoot(num));
    }
    public static double squareRoot(int num){
        double temp;
        double sqrt=num/2;
        do {
            temp=sqrt;
            sqrt=(temp+(num/temp))/2;
        }while ((temp-sqrt)!=0);
        return sqrt;
    }
