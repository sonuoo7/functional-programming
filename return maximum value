import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
  public static int maximumInt(int a,int b,int c){
    if (a>b&&a>c){
      return a;
    }
    else if (b>a&&b>c){
      return b;
    }
    else  return c;
  }
	public static void main (String[] args) throws java.lang.Exception
	{
      int a,b,c;
		Scanner sc=new Scanner(System.in);
      a=sc.nextInt();
      b=sc.nextInt();
      c=sc.nextInt();
      System.out.println(maximumInt(a,b,c));
	}
}
