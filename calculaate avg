import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
  public static int avgCalc(int arr[],int n){
     int sum=0;
      for (int i=0;i<n;i++){
       sum=arr[i]+sum;
        
      }
      int avg=sum/arr.length;
    return avg;
  }
	public static void main (String[] args) throws java.lang.Exception
	{
      
		Scanner sc=new Scanner(System.in);
             int n=sc.nextInt();
      int arr[]=new int[n];
      for(int i=0;i<n;i++){
        arr[i]=sc.nextInt();
      }
      System.out.println(avgCalc(arr,n));
    }
}
