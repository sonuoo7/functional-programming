import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
  public static long Fact(long num){
    long fact=1;
   int i=1;
    while (i<=num){
      fact*=i;
      i++;
    }
    return fact;
  }
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc=new Scanner(System.in);
      long num=sc.nextInt();
     
        System.out.println(Fact(num));
      
	}
}
